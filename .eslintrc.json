{
    "env": {
        "es2021": true,
        "node": true,
    },
    "extrends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
    },
    "rules": {
        "prettier/prettier": "error",
        "class-methods-use-this": "off",
        "camelcase": "off"
    },
}